@{
    Layout = null;
}
@using JCJ.OA.Model
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>视频播放</title>
</head>
<body>
    @{
        VideoFileInfo videoFileInfo = ViewBag.VideoInfo;
    }
    <div>
        <h1>@videoFileInfo.Title</h1>
        <script src='/VideoPlay/AC_RunActiveContent.js' language='javascript'></script>
        <!-- saved from url=(0013)about:internet -->
        <script language='javascript'>
    AC_FL_RunContent('codebase', 'http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=9,0,0,0', 'width', '400', 'height', '325', 'src', ((!DetectFlashVer(9, 0, 0) && DetectFlashVer(8, 0, 0)) ? '/VideoPlay/OSplayer' : '/VideoPlay/OSplayer'), 'pluginspage', 'http://www.macromedia.com/go/getflashplayer', 'id', 'flvPlayer', 'allowFullScreen', 'true', 'allowScriptAccess', 'always', 'movie', ((!DetectFlashVer(9, 0, 0) && DetectFlashVer(8, 0, 0)) ? '/VideoPlay/OSplayer' : '/VideoPlay/OSplayer'), 'FlashVars', 'movie=@videoFileInfo.VideoPath&btncolor=0x333333&accentcolor=0x31b8e9&txtcolor=0xdddddd&volume=30&autoload=on&autoplay=off&vTitle=Super Mario Brothers Lego Edition&showTitle=yes');
        </script>
        <noscript>
            <object width='400' height='325' id='flvPlayer'>
                <param name='allowFullScreen' value='true'>
                <param name="allowScriptAccess" value="always">
                <param name='movie' value='/VideoPlay/OSplayer.swf?movie=@videoFileInfo.VideoPath&btncolor=0x333333&accentcolor=0x31b8e9&txtcolor=0xdddddd&volume=30&autoload=on&autoplay=off&vTitle=Super Mario Brothers Lego Edition&showTitle=yes'>
                <embed src='/VideoPlay/OSplayer.swf?movie=@videoFileInfo.VideoPath&btncolor=0x333333&accentcolor=0x31b8e9&txtcolor=0xdddddd&volume=30&autoload=on&autoplay=off&vTitle=Super Mario Brothers Lego Edition&showTitle=yes' width='400' height='325' allowFullScreen='true' type='application/x-shockwave-flash' allowScriptAccess='always'>
            </object>
        </noscript>

    </div>
</body>
</html>
